					location.reload();
				}


			},
			error : function(result) {
				// console(result);

			}
		});
	} else
		alert("请把信息填写完整");

}

function md5_password(password){
  var md5Password = hex_md5(password);
      md5Password += "DNF";
      md5Password = hex_md5(password);
      return md5Password;
}


